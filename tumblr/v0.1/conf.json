{
  "displayName":"Tumblr",
  "icon":"",
  "base_url":"api.tumblr.com/v2/",
  "default_auth":"manual",
  "auth":{
    "none":{},
    "manual":{
      "request":{
        "get":{
          "api_key":"{client_secret}"
        }
      },
      "parameters":[
        {
          "name":"client_secret",
          "type":"string",
          "required":false
        }
      ]
    }
  }
}