{ 
	"displayName":"Flickr",
	"icon":"",
	"base_url":"",
  "default_auth":"oauth1",
  "auth":{
    "none":{},
    "oauth1":{
      "request_token":{
        "url":"http://www.flickr.com/services/oauth//request_token",
        "params":{
          "oauth_consumer_key":"{client_id}",
          "oauth_callback":"http://{_wsdomain}.webshell.local/name/{_name}?api={_id}&cid={_cid}"
        }
      },
      "authorize":{
        "url":"http://www.flickr.com/services/oauth/authorize"
      },
      "access_token":{
        "url":"http://www.flickr.com/services/oauth/access_token",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "request":{
        "format":"json",
	"get":{
		"api_key":"{client_id}"
	},
	"put":{
		"api_key":"{client_id}"
	},
	"post":{
		"api_key":"{client_id}"
	},
	"delete":{
		"api_key":"{client_id}"
	},
        "params":{
         "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        }
      ]
    }
  }
}
