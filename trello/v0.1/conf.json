{ 
	"displayName":"Trello",
	"icon":"",
	"base_url":"https://api.trello.com/1/",
	"default_auth":"oauth1",
	"auth":{ 
		"none":{}, 
		    "oauth1":{
		    "request_token":{
			"url":"https://trello.com/1/OAuthGetRequestToken",
        		"params":{
          		"oauth_consumer_key":"{client_id}",
          		"oauth_callback":"{_callback_url}?{_callback_args}&scope={scope}" 
        	}
      },
      "access_token":{
        "url":"https://trello.com/1/OAuthGetAccessToken",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "authorize":{
        "url":"https://trello.com/1/OAuthAuthorizeToken"
      },
      "request":{
	"get":{
		"key":"{client_id}"
	},
        "format":"json",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        },
        {
          "name":"scope",
          "type":"string"
        }
      ]
    }
  }
}