{
  "displayName":"Deezer",
  "icon":"",
  "base_url":"",
  "default_auth":"none",
  "auth": {
    "none":{},
    "oauth2": {
      "authorize": {
        "url":"https://connect.deezer.com/oauth/auth.php",
        "format":"url",
        "params": {
          "app_id":"{app_id}",
          "redirect_uri":"http://{_wsdomain}.webshell.local/name/{_name}?api={_id}&cid={_cid}"
	  }
      },
      "access_token":{
        "url":"https://connect.deezer.com/oauth/access_token.php",
        "format":"url",
        "field":"state",
        "params":{
	  "app_id":"{app_id}",
          "secret":"{secret}",
 	  "code":"{_code}"
        }
      },
      "request":{
	    "format":"json",
        "get":{
          "oauth_token":"{_access_token}"
        }
      },
      "parameters":[
        {
          "name":"app_id",
          "type":"string"
        },
        {
          "name":"secret",
          "type":"string"
        }
      ]
    }
  }
}