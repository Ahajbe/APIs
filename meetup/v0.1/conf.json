{
  "displayName":"MeetUp",
  "icon":"",
  "base_url":"",
  "default_auth":"oauth2",
  "auth": {
    "oauth2": {
      "authorize": {
        "url":"https://secure.meetup.com/oauth2/authorize",
        "params": {
          "client_id":"{client_id}",
          "response_type":"code",
          "redirect_uri":"{_callback_url}",
          "state":"{_callback_args}",
	  "scope":"{scope}"
        }
      },
      "access_token":{
        "url":"https://secure.meetup.com/oauth2/access",
        "field":"state",
        "method":"post",
        "params":{
          "client_id":"{client_id}",
          "client_secret":"{client_secret}",
          "redirect_uri":"{_callback_url}",
          "grant_type":"authorization_code",
          "code":"{_code}"
        }
      },
      "request":{
	    "format":"json",
        "get":{
          "oauth_token":"{_access_token}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        },
        {
          "name":"scope",
          "type":"string",
	"separator":" ",
	"cardinality":"*",
	"values": {
	"basic":"Access to Meetup group info, Everywhere API, creating and editing Events and RSVP's, posting photos",
	"messaging":"Enables Member to Member messaging",
	"ageless":"Replaces the one hour expiry time from oauth2 tokens with a limit of up to two weeks"
		  }
        }
      ]
    }
  }
}