{ 
	"displayName":"Linkedin",
	"icon":"",
	"base_url":"https://api.linkedin.com/uas/oauth",
	"default_auth":"oauth1",
	"auth":{ 
		"none":{}, 
		    "oauth1":{
		    "request_token":{
			"url":"https://api.linkedin.com/uas/oauth/requestToken",
        		"params":{
          		"oauth_consumer_key":"{client_id}",
          		"oauth_callback":"http://{_wsdomain}.webshell.local/name/{_name}?api={_id}&cid={_cid}"
        	}
      },
      "access_token":{
        "url":"https://api.linkedin.com/uas/oauth/accessToken",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "authorize":{
        "url":"https://api.linkedin.com/uas/oauth/authenticate"
      },
      "request":{
        "format":"json",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        }
      ]
    }
  }
}