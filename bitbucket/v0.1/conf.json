{
  "displayName":"BitBucket",
  "icon":"",
  "base_url":"",
  "default_auth":"oauth1",
  "auth":{
    "none":{},
    "oauth1":{
      "request_token":{
        "url":"https://bitbucket.org/api/1.0/oauth/request_token/",
        "params":{
          "oauth_consumer_key":"{client_id}",
          "oauth_callback":"{_callback_url}?{_callback_args}"
           }
      },
      "authorize":{
        "url":"https://bitbucket.org/api/1.0/oauth/authenticate/"
      },
      "access_token":{
        "url":"https://bitbucket.org/api/1.0/oauth/access_token/",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "request":{
        "format":"json",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        }
     ]
    }
  }
}