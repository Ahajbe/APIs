{ 
	"displayName":"Skyrock",
	"icon":"",
	"base_url":"",
  "auth":{
    "none":{},
    "oauth1":{
      "request_token":{
        "url":" https://api.skyrock.com/v2/oauth/initiate",
        "params":{
          "oauth_consumer_key":"{client_id}",
          "oauth_callback":"http://{_wsdomain}.webshell.local/name/{_name}?api={_id}&cid={_cid}"
        }
      },
      "authorize":{
        "url":"https://api.skyrock.com/v2/oauth/authorize"
      },
      "access_token":{
        "url":"https://api.skyrock.com/v2/oauth/token",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "request":{
        "format":"json",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        }
      ]
    }
  }
}
